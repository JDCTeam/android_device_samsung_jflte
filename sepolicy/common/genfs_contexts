# proc labels
genfscon proc /bluetooth/sleep                u:object_r:proc_bt_sleep:s0

# sysfs labels
genfscon sysfs /devices/system/soc/soc0/id    u:object_r:sysfs_soc_id:s0
genfscon sysfs /devices/virtual/camera        u:object_r:sysfs_camera:s0
genfscon sysfs /kernel/cpufreq_limit          u:object_r:sysfs_cpufreq_limit:s0
